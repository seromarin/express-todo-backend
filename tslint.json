{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "quotemark": [
            true,
            "single"
        ],
        "no-bitwise": true,
        "await-promise": true,
        "no-eval": true,
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        }
    },
    "rulesDirectory": []
}